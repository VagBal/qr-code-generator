{
  "version": 2,
  "builds": [
    {
      "src": "qrcodegen/app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "qrcodegen/app.py"
    }
  ],
  "functions": {
    "qrcodegen/app.py": {
      "runtime": "python3.12",
      "includeFiles": ["libjpeg.so.62"]
    }
  }
}
