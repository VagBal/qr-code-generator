{
  "version": 2,
  "builds": [
    {
      "src": "qrcodegen/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "qrcodegen/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12.0"
  },
  "build": {
    "env": {
      "LD_LIBRARY_PATH": "/usr/local/lib"
    },
    "commands": [
      "apt-get update",
      "apt-get install -y libjpeg62"
    ]
  }
}
